<header class="navbar">
   
    <div class="navbar-brand">
        <img src="<%= logoUrl %>" alt="Logo Admin" class="admin-logo-img">
        <h1>Gestor de Productos</h1>
    </div>
    
    <nav>
        <ul>
            <li><a href="/">Inicio</a></li>
            <li><a href="/cargar">Cargar Producto</a></li>
            <li><a href="/buscar">Buscar por ID</a></li>
            <li><a href="/modificar">Modificar Producto</a></li>
            <li><a href="/eliminar">Eliminar Producto</a></li>
        </ul>
    </nav>
    
    <div class="alumnos-info">
        <strong>Alumnos:</strong><br>
        
        <% alumnos.forEach(alumno => { %>
        
            <%= alumno.nombre %> <%= alumno.apellido %><br>
        
        <% }) %>
    </div>

    <form action="/logout" method="POST" id="logout">
        <input type="submit" value="Cerrar sesion" id="btn-logout">
    </form>

</header>